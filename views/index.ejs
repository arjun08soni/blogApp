<% include partials/header%>
<h1>Blog Page</h1>
<% blogs.forEach((blog)=>{%>
<div>
	<h2><%= blog.title%></h2>
	<img src="<%= blog.image%>">
	<span><%= blog.created%></span>
	<p><%= blog.body.substring(0,101)%>...</p>
	<a href="/blogs/<%= blog._id%>">Read More</a>
</div>
<% }); %>
<% include partials/footer%>